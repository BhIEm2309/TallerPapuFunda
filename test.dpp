function sumar(integer a, integer b) {
    integer resultado;
    resultado = a + b;
    print "La suma es:";
    print resultado;
    return resultado;
}

function restar(integer a, integer b) {
    integer resultado;
    resultado = a - b;
    print "La resta es:";
    print resultado;
    return resultado;
}

function multiplicar(integer a, integer b) {
    integer resultado;
    resultado = a * b;
    print "La multiplicación es:";
    print resultado;
    return resultado;
}

function dividir(integer a, integer b) {
    integer resultado;
    resultado = a / b;
    print "La división es:";
    print resultado;
    return resultado;
}

integer x;
integer y;

print "Ingrese el primer número:";
write x;

print "Ingrese el segundo número:";
write y;

sumar(x, y);
restar(x, y);
multiplicar(x, y);
dividir(x, y);
